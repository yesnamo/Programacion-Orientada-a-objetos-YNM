public class Main {
    int x;
    
    public Main() {
        x = 5;
    }

    public class Cod {
        int y;

        public Cod() {
            y = 0;
        }
    }

    public static void main(String[] args) {

        Main myObj1 = new Main();

        Cod myObj2 = new Main().new Cod();

        System.out.println(" x = " + myObj1.x + " constructor con parametros");
        System.out.println(" y = " + myObj2.y + " constructor vacio");
    }
}
